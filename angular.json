{
  "version": 1,
  "projects": {
    "calculator-display": {
      "projectType": "library",
      "root": "libs/calculator-display",
      "sourceRoot": "libs/calculator-display/src",
      "prefix": "mortgage-calculator",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/calculator-display"],
          "options": {
            "jestConfig": "libs/calculator-display/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/calculator-display/src/**/*.ts",
              "libs/calculator-display/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "calculator-ui": {
      "projectType": "application",
      "root": "apps/calculator-ui",
      "sourceRoot": "apps/calculator-ui/src",
      "prefix": "mortgage-calculator",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/calculator-ui",
            "index": "apps/calculator-ui/src/index.html",
            "main": "apps/calculator-ui/src/main.ts",
            "polyfills": "apps/calculator-ui/src/polyfills.ts",
            "tsConfig": "apps/calculator-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/calculator-ui/src/favicon.ico",
              "apps/calculator-ui/src/assets"
            ],
            "styles": ["apps/calculator-ui/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/calculator-ui/src/environments/environment.ts",
                  "with": "apps/calculator-ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "calculator-ui:build:production"
            },
            "development": {
              "browserTarget": "calculator-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "calculator-ui:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/calculator-ui/src/**/*.ts",
              "apps/calculator-ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/calculator-ui"],
          "options": {
            "jestConfig": "apps/calculator-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "calculator-ui-e2e": {
      "root": "apps/calculator-ui-e2e",
      "sourceRoot": "apps/calculator-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/calculator-ui-e2e/cypress.json",
            "devServerTarget": "calculator-ui:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "calculator-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/calculator-ui-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["calculator-ui"]
    }
  }
}
