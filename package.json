{
  "name": "mortgage-calculator",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "test:integration:calculator-api": "node_modules/.bin/newman run docs/Postman/MortgageCalculator.postman_collection.json -e docs/Postman/Mortgage-Calculator.postman_environment.json ",
    "test:integration:mortgage-calculator-api": "node_modules/.bin/newman run docs/Postman/Mortgage-Calculation-API.postman_collection.json -e docs/Postman/Mortgage-Calculator.postman_environment.json ",
    "affected:lint": "nx affected:lint --fix",
    "affected:test": "nx affected --target=test --base=main --head=HEAD",
    "test:all": "nx run-many --all --target=test",
    "lint:all": "nx run-many --all --target=lint",
    "coverage:all": "nx run-many --all --target=test --codeCoverage",
    "docker:build:calculator-api": "docker build -f apps/calculator-api/Dockerfile -t shannonlal/calculator-api:\"$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')\" .",
    "docker:push:calculator-api": "docker push shannonlal/calculator-api:\"$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')\"",
    "docker:build:mortgage-calculator-api": "docker build -f apps/mortgage-calculation-api/Dockerfile -t shannonlal/mortgage-calculation-api:\"$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')\" .",
    "docker:push:mortgage-calculator-api": "docker push shannonlal/mortgage-calculation-api:\"$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]')\"",
    "mongo:start":"ts-node mongo-memory.ts"  
  },
  "private": true,
